<p id="notice"><%= notice %></p>

<h1>Muscle Groups</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>region_id</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @muscle_groups.each do |muscle_group| %>
      <tr>
        <td><%= muscle_group.muscle_title %></td>
        <td><%= muscle_group.region_id %></td>
        <td><%= link_to 'Show', muscle_group %></td>
        <td><%= link_to 'Edit', edit_muscle_group_path(muscle_group) %></td>
        <td><%= link_to 'Destroy', muscle_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Muscle Group', new_muscle_group_path %>
