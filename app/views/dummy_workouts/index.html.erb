<p id="notice"><%= notice %></p>

<h1>Dummy Workouts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Program day</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dummy_workouts.each do |dummy_workout| %>
      <tr>
        <td><%= dummy_workout.user %></td>
        <td><%= dummy_workout.title %></td>
        <td><%= dummy_workout.program_day %></td>
        <td><%= link_to 'Show', dummy_workout %></td>
        <td><%= link_to 'Edit', edit_dummy_workout_path(dummy_workout) %></td>
        <td><%= link_to 'Destroy', dummy_workout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dummy Workout', new_dummy_workout_path %>
